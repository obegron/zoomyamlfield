{
  "name": "vscode-yaml-field-editor",
  "displayName": "YAML Field Editor",
  "description": "Zoom and edit YAML fields",
  "publisher": "obegron",
  "version": "0.0.4",
  "repository": "git@github.com:obegron/zoomyamlfield.git",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.zoomYamlField",
        "title": "Zoom edit YAML Field"
      },
      {
        "command": "extension.exitZoomYamlField",
        "title": "Exit Zoom edit YAML Field"
      },
      {
        "command": "extension.activateYamlKey",
        "title": "Zoom edit YAML Field"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == yaml",
          "command": "extension.activateYamlKey",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "22.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^9.10.0",
    "glob": "^11.0.0",
    "mocha": "^10.0.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}